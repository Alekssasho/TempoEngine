namespace Tempest.Definition;

enum ShaderType : byte
{
    Vertex,
    Pixel
}

table Shader
{
    Name : string(key);
    Type : ShaderType;
    Code : [ubyte];
}

table ShaderLibrary
{
    Shaders : [Shader];
}


file_identifier "TSLB";
file_extension "tslb";
root_type ShaderLibrary;